pipeline{
    agent any

    stages{

        stage('Cleanup workspace'){
            
            steps{
                script{
                    cleanWs()
                }
            }
        }
       stage('checkout SCM'){
            steps{
                script{
                    git credentialsId: 'github',
                    url: 'https://github.com/lehuy94/DevOpsBK.git',
                    branch: 'master'
                }
            }
        }
    }
}
